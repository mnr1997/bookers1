<h1>Books</h1>

  <table>
    <thead>
      <tr>
        <th>Title</th><th>Body</th><th></th><th></th><th></th>
      </tr>
    </thead>
    <tbody>
      <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
        <td><%= link_to "Show", book_path(book.id) %></td>
        <td><%= link_to "Edit", book_path(book.id) %></td>
        <td><%= link_to "Destroy", book_path(book.id) %></td>
      <% end %>
      </tr>
    </tbody>
  </table>



